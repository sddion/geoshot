  "compilerOptions": {
    "jsx": "react",
    "baseUrl": ".",
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ],
      "@rork-ai/toolkit-sdk": [
        "@rork-ai/toolkit-sdk"
      ]
    }
  },
  "include": [
